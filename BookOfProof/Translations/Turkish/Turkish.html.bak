}

/* The container <div> - needed to position the dropdown content */
.dropdown {
    position: relative;
    display: inline-block;
}

/* Dropdown Content (Hidden by Default) */
.dropdown-content {
  
    display: none;
    font-size: 12px;
    padding: 4px 4px;
    position: absolute;
    background-color: white;
    min-width: 300px;
    box-shadow: 10px 10px 5px 0px rgba(0,0,0,0.2);
